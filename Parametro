
programa {

  funcao menor_numero(inteiro n1, inteiro n2)
  {
    se (n1 < n2)
    {
      retorne n1
    }
    senao
    {
      retorne n2
    }
  }

  funcao inicio() 
  {

    inteiro n1, n2, menor

    escreva ("informe n° para n1: ")
    leia (n1)
    escreva ("informe n° para n2: ")
    leia (n2)

    menor = menor_numero(n1, n2)

    escreva(menor)
  }
}
